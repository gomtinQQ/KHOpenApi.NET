<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<LangVersion>10.0</LangVersion>
		<TargetFrameworks>netstandard2.0</TargetFrameworks>
		<PlatformTarget>AnyCPU</PlatformTarget>
		<Description>Common wrapper for Kiwoom OpenApi</Description>
		<Version>1.4.0</Version>
		<Title>KHOpenAPI.NET</Title>
		<PackageTags>kiwoom, KOA</PackageTags>
		<Authors>teranum</Authors>
		<Copyright>Copyright Â© teranum 2022</Copyright>
		<PackageProjectUrl>https://github.com/terapark/KHOpenAPI.NET</PackageProjectUrl>
		<PackageIcon>icon.png</PackageIcon>
		<Company>$(Authors)</Company>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
		<None Include="icon.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
